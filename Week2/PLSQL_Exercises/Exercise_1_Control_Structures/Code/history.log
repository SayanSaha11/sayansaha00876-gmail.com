b03a462b-c4b2-4451-ba7a-545f4833a4da:1750505362747:114:S:BEGIN\n  FOR c IN (\n    SELECT c.CustomerID, TRUNC(MONTHS_BETWEEN(SYSDATE, c.DOB) / 12) AS Age\n    FROM Customers c\n  ) LOOP\n    IF c.Age > 60 THEN\n      UPDATE Loans\n      SET InterestRate = InterestRate - 1\n      WHERE CustomerID = c.CustomerID;\n    END IF;\n  END LOOP;\n  COMMIT;\nEND;\n/
